'САБ =======================================================
'езда от Мусорки до первого устр-ва умного дома потом до 1-ой белой лампы и возврат на линию
Sub bellam
  
  a3()
  a2()
  While S3>15 And S2>15 Or S3<15 And S2<15 ' доезд до линии
    a3()
    a2()
    speed=30
    polin2d()
  EndWhile
  Motor.Stop("bc","true")
  pr()
  Motor.ResetCount("bc")
  While (Motor.GetCount("b") + Motor.GetCount("c") )/2<200
    speed=20
    polin2d()
  EndWhile
  Motor.Stop("bc","true")
  a3()
  a2()
  For i=1 To 1
    k1=0.2
    k2=0
    
    a3()
    a2()
    While S3>15 And S2>15 Or S3<15 And S2<15 ' доезд до линии
      a3()
      a2()
      speed=30
      polin2d()
    EndWhile
    Motor.Stop("bc","true")
    k1=0.15
    k2=8
    Motor.MoveSync("bc",30,30,50,"true")
  EndFor
  Motor.Stop("bc","true")
  a3()
  a2()
  j=80
  While S3>15
    a3()
    a2()
    If j>30 Then
      j=j-0.1
    EndIf
    Motor.StartSync("bc",20,-20)
  EndWhile
  Motor.Stop("bc","true")
  a3()
  a2()
  j=80
  While S3<25
    a3()
    a2()
    
    If j>30 Then
      j=j-0.1
    EndIf
    Motor.StartSync("bc",20,-20)
  EndWhile
  Motor.Stop("bc","true")
  Motor.ResetCount("bc")
  While (Motor.GetCount("b")+Motor.GetCount("c"))/2<50
    a3()
    a2()
    speed=30
    a3()
    a2()
    e=S3-((min3+max3)/2)
    u=k1*e+k2*(e-eold)
    eold=e
    Speedl=speed+u
    Speedr=speed-u
    Motor.StartPower("B",Speedl)
  EndWhile
  Motor.Stop("bc","true")
  Motor.MoveSync("bc",30,30,70,"true")
  
  Motor.Start("d",50) ' ==== забрали 1-ую белую лампу и поехали на отвоз ее ===================
  Program.Delay(500)
  Motor.Stop("bc","true")
  Motor.MoveSync("bc",-30,-30,50,"true")
  Motor.MoveSync("bc",20,0,350,"true")
  a2()
  a3()
  'отъезд назад до черной линии
  While S3>10 And S2>10 Or S3<10 And S2>10 Or S3>6 And S2<6 'отъезд
    a2()
    a3()
    Motor.StartPower("bc",20)
  EndWhile
  Motor.Stop("bc","true")
  Motor.MoveSync("bc",30,30,50,"true")
  Motor.Stop("bc","true")
  a3()
  a2()
  While S3>10
    a3()
    a2()
    Motor.StartSync("bc",20,-20)
  EndWhile
  Motor.Stop("bc","true")
  
  a3()
  a2()
  While S3<60
    a3()
    a2()
    Motor.StartSync("bc",20,-20)
  EndWhile
  Motor.Stop("bc","true")
  Program.Delay(100)
EndSub

