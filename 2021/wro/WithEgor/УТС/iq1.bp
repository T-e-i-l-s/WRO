' ===============================================================
' САБ езда прямо до линии с ВЫРАВНИВАНИЕМ по линии !!!!
Sub Virav
  Sensor.SetMode(2,0)
  Sensor.SetMode(3,0)
  While Sensor.ReadPercent(2)>6 And Sensor.ReadPercent(3)>6
    Motor.StartSync("BC",20,20)
  EndWhile
  Motor.Stop("BC","True")
  While Sensor.ReadPercent(2)>6
    Motor.Start("B",20)
  EndWhile
  Motor.Stop("B","True")
  While Sensor.ReadPercent(3)>6
    Motor.Start("C",20)
  EndWhile
  Motor.Stop("C","True")
  
  While Sensor.ReadPercent(2)<6
    Motor.Start("B",-20)
  EndWhile
  Motor.Stop("B","True")
  While Sensor.ReadPercent(3)<6
    Motor.Start("C",-20)
  EndWhile
  Motor.Stop("C","True")
EndSub

